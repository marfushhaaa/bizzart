<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/wall3_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/wall3_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/wall3_darktheme3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/black3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/wall3_image"
        app:layout_constraintTop_toTopOf="@+id/wall3_image"
        android:src="@drawable/new_black" />

    <ImageView
        android:id="@+id/bDiary"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="276dp"
        android:src="@drawable/binvisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.863"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/butt_left3"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="195dp"
        android:src="@drawable/bleftwhite"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/butt_right3"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="70dp"
        android:src="@drawable/brightwhite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>